# Tetracore Server - Backend Environment Variables

# Database Configuration
MONGO_URL="mongodb://localhost:27017"
DB_NAME="tetracore_db"

# Server Configuration  
HOST="0.0.0.0"
PORT=8001

# CORS Configuration
CORS_ORIGINS="*"

# Security (untuk production)
# SECRET_KEY="your-secure-secret-key-here"
# JWT_SECRET="your-jwt-secret-here"

# Logging
LOG_LEVEL="INFO"

# MongoDB Authentication (jika menggunakan auth)
# MONGO_USERNAME="tetracore_user"
# MONGO_PASSWORD="secure_password"

# Production Database (jika berbeda)
# MONGO_URL_PROD="mongodb://user:pass@localhost:27017/tetracore_prod"